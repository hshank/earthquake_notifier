<?xml version="1.0" encoding="utf-8"?>
<android.support.wearable.view.BoxInsetLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/container"
    tools:context=".MainActivity"
    tools:deviceIds="wear"
    android:layout_gravity="bottom">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >


        <Button

            style="?android:attr/buttonStyleSmall"
            android:layout_width="167dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|bottom"
            android:text="Return to Quake \nList"
            android:id="@+id/button"
            android:textColor="#35478C"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:background="#eaea14"
                />

        <TextView android:id="@+id/textid" app:layout_box="all" android:layout_width="186dp"
            android:layout_height="87dp" android:text="SHAKE TO ACCESS INSTAGRAM"
            android:layout_gravity="center"
            android:textColor="#35478C"
            android:gravity="center"/>

    </FrameLayout>

</android.support.wearable.view.BoxInsetLayout>
